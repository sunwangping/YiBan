.biaoti {
  width: 200rpx;
  height: 50rpx;
  padding: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #ff5e5b;
  border-radius: 15rpx;
  margin: 30rpx;
}
.Active2 {
  line-height: 25px;
  font-size: 30rpx;
  margin: 25rpx 30rpx;
  display: flex;
  flex-flow: column;
  justify-content: center;
  border-radius: 15px;
  width: 700rpx;
  height: auto;
  background-color: #fff;
}
.Active2 .Active3 {
  display: flex;
  align-items: center;
  margin: 20rpx 30rpx;
  padding-left: 20rpx;
  width: 600rpx;
  height: auto;
  border-left: 5px solid #ff5e5b;
}
.Active2 .Active4 {
  display: flex;
  align-items: center;
  margin: 20rpx 30rpx;
  padding-left: 20rpx;
  width: 600rpx;
  height: auto;
  border-left: 5px solid #18c3b3;
}
.gongneng_cata {
  display: flex;
  margin: 50rpx 30rpx;
  margin-bottom: 0rpx;
}
.gongneng_cata navigator {
  display: flex;
  justify-content: center;
  flex: 1;
}
.gongneng_cata navigator image {
  width: 60%;
  height: 60%;
}
.name_list {
  display: flex;
  margin: 0rpx 30rpx;
  margin-bottom: 30rpx;
}
.name_list .name {
  font-size: 30rpx;
  display: flex;
  flex: 1;
  justify-content: center;
}
.all {
  height: 100rpx;
  display: flex;
  align-items: flex-end;
  justify-content: space-between;
}
.all .more {
  margin-right: 60rpx;
  margin-bottom: 20rpx;
}
.zero {
  display: flex;
  flex-flow: column;
  height: 110rpx;
  font-size: 35rpx;
  margin: 20rpx 30rpx;
  background-color: #fff;
  border-radius: 15rpx;
}
.zero .first {
  color: #000;
  display: flex;
  justify-content: space-between;
}
.zero .first .earing_info {
  flex: 8;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-top: 20rpx;
  margin-right: 30rpx;
}
.zero .first .icon2 {
  flex: 1;
  margin-top: 0rpx;
  font-size: 35rpx;
  color: #ff5e5b;
}
.zero .time {
  display: flex;
  margin-right: 30rpx;
  justify-content: flex-end;
  color: #999592;
  font-size: 30rpx;
}
.second {
  display: flex;
  align-items: center;
}
.icon1 {
  flex: 1;
  margin-left: 20rpx;
  margin-top: 15rpx;
  font-size: 50rpx;
  color: #ff5e5b;
}
.icon3 {
  flex: 1;
  margin-left: 20rpx;
  margin-top: 15rpx;
  font-size: 50rpx;
  color: #18c3b3;
}
.content {
  margin: 25rpx 30rpx;
  display: flex;
  justify-content: center;
  border-radius: 5px;
  width: 700rpx;
  height: auto;
  background-color: #fff;
}
.content .befor {
  display: flex;
  margin: 20rpx 30rpx;
  padding-left: 20rpx;
  width: 600rpx;
  height: auto;
  border-left: 5px solid #999592;
  color: #999592;
}
.content .befor .after {
  font-size: 16px;
  line-height: 24px;
}

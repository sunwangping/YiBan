<view wx:if="{{Collected}}">
  <view wx:for="{{Collected}}" wx:key="id">
  <view class="zero1" >
    <view class="first1">
        <view class="second">
          <icon class="icon1_1 iconfont icon-yuandian"></icon>
          <text class="earing_info1">{{item.title}}</text>
        </view>
        <icon data-index="{{index}}" catchtap="Leaving" class="icon2_1 iconfont icon-collection-b"></icon>
    </view>
    <view class="time">{{item.date}}</view>
  </view>
</view>
</view>
  
<view wx:else>
  <text class="font">
    您还没有收藏任何活动噢
  </text>
    
</view>
  
